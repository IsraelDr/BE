        <Button x:Name="MotherButton" Content="Mother" Margin="61,11,49.2,9.8" Height="56" Grid.Column="1" Click="MotherButton_Click"/>
        <Button x:Name="NannyButton" Content="Nanny" Margin="61,10.2,49.2,19.4" Height="56" Grid.Column="1" Grid.Row="1" Click="NannyButton_Click"/>
        <Button x:Name="ChildButton" Content="Child" Margin="61,13.6,49.2,10.4" Height="56" Grid.Column="1" Grid.Row="2" Click="ChildButton_Click"/>
        <Button x:Name="ContractsButton" Content="Contract" Margin="61,12.6,49.2,9.8" Height="56" Grid.Column="1" RenderTransformOrigin="0.615,4.838" Grid.Row="3" Click="ContractsButton_Click"/>

    </Grid>
</Window>
