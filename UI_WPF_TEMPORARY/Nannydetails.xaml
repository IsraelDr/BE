            <TextBox HorizontalAlignment="Left" Name="new_Tuesday_end" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="2" Grid.Row="2"/>
            <TextBox HorizontalAlignment="Left" Name="new_Wednesday_start" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="1" Grid.Row="3"/>
            <TextBox HorizontalAlignment="Left" Name="new_Wednesday_end" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="2" Grid.Row="3"/>
            <TextBox HorizontalAlignment="Left" Name="new_Thursday_start" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="1" Grid.Row="4"/>
            <TextBox HorizontalAlignment="Left" Name="new_Thursday_end" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="2" Grid.Row="4"/>
            <TextBox HorizontalAlignment="Left" Name="new_Friday_start" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="1" Grid.Row="5"/>
            <TextBox HorizontalAlignment="Left" Name="new_Friday_end" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="2" Grid.Row="5"/>
            <TextBox HorizontalAlignment="Left" Name="new_Saturday_start" Height="20" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="82" Grid.RowSpan="2" Grid.Column="1" Grid.Row="6"/>
