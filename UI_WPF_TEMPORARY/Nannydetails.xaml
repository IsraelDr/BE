            <CheckBox Content="Tuesday" Name="new_Tuesday" HorizontalAlignment="Left"  VerticalAlignment="Top" Grid.Row="1" Grid.RowSpan="2" Margin="0,19.8,0,0"/>
            <CheckBox Content="Wednesday" Name="new_Wednesday" HorizontalAlignment="Left"  VerticalAlignment="Top" Grid.Row="3" Margin="0,0.2,0,0"/>
